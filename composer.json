{
    "name": "I18n",
    "description": "Internationalization plugin for BEdita 4 & CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Documents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "I18n\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
